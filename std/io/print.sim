import std::c
import std::reflect

// 格式化输出
pub func print(s: str)Result::<usize, ()>{
    let header = *(&s as *reflect::string)
    let count = c::printf(header.ptr as c::string)
    if count < 0{
        return Err::<usize, ()>(())
    }
    return Ok::<usize, ()>(count as usize)
}

// 格式化输出带换行
pub func println(s: str)Result::<usize, ()>{
    let res = print(s)
    print("\n")
    return res
}
