import std::c

// 格式化输出
pub func print(s: str)Result::<usize, ()>{
    let count = c::printf(c::str2cstring(s))
    if count < 0{
        return Err::<usize, ()>(())
    }
    return Ok::<usize, ()>(count as usize)
}

// 格式化输出带换行
pub func println(s: str)Result::<usize, ()>{
    let count = c::puts(c::str2cstring(s))
    if count < 0{
        return Err::<usize, ()>(())
    }
    return Ok::<usize, ()>(count as usize)
}
