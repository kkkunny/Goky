struct pair<T, F>{
    pub F: T,
    pub S: F,
}

func (self: pair<T, F>) only_first()T{
    return self.F
}

func (self: pair<T, F>) first()T{
    return self.first2()
}

func (self: pair<T, F>) first2()T{
    let a = pair::<T, F>{F: self.F, S: self.S}
    return a.only_first()
}

func main()u8{
    let a = pair::<u8, u8>{F: 1, S: 2}
    return a.first() - 1
}