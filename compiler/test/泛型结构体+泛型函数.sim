struct either<T, F>{
    pub isLeft: bool,
    pub Left: T,
    pub Right: F
}

struct pair<T, F>{
    pub Value: either::<T, F>,
}

func new_pair<T, F>(x: T, y: F)pair::<T, F>{
    return pair::<T, F>{Value: either::<T, F>{isLeft: true, Left: x, Right: y}}
}

func main()u8{
    let a = new_pair::<u8, i16>(1, 2)
    return a.Value.Left - 1
}